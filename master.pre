break;
case 'B':
case 'b':
case 'C':
case 'c':
case '2':
case '5':
case '6':
BuildCmd(code, 0, args);
break;
case 'G':
case 'g':
case '4':

BuildCmd(code, 0, args);

stat.WaitResp = TRUE;
break;
case '1':
BuildCmd(code, 2, args);
break;
}
}



