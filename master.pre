void ChipSelect(int dev)
{
SpiLock = 1;

PORTBbits.RB4 = 1;
PORTCbits.RC0 = 1;
PORTDbits.RD6 = 1;
PORTDbits.RD7 = 1;

switch (dev) {
case 0:
PORTCbits.RC0 = 0;
break;
case 1:
PORTDbits.RD6 = 0;
break;
case 2:
PORTDbits.RD7 = 0;
break;
case 3:
PORTBbits.RB4 = 0;
break;
default:
break;
}

}


void ChipReserve(int dev)
{
SpiLock = 1;

PORTBbits.RB4 = 1;
PORTCbits.RC0 = 1;
PORTDbits.RD6 = 1;
PORTDbits.RD7 = 1;
}



